<div id="main" class="<%= item.layout %>" itemscope itemprop="blogPost">
	<% if (page.layout=='photo' && item.photos && item.photos.length){ %>
	<%- partial('gallery') %>
	<% } %>
	<article itemprop="articleBody">
		<%- partial('header') %>
		<div class="article-content">
			<% if( table&&(item.toc !== false) && theme.toc.article){ %>
			<div id="toc" class="toc-article" <% if (item.encrypt == true) { %>style="display:none" <% } %>>
				<strong class="toc-title"><%= __('contents') %></strong>
				<!-- <% if(item.list_number == false) {%>
				<%- toc(item.content,{list_number:false}) %>
				<% }else{ %>
				<%- toc(item.content) %>
				<% } %> -->
				<% if (item.encrypt == true) { %>
				<%- toc(item.origin, {list_number: item.list_number || true}) %>
				<% } else { %>
				<%- toc(item.content, {list_number: item.list_number || true}) %>
				<% } %>
			</div>
			<% } %>
			<%- item.content %>
		</div>
		<%- partial('footer') %>
	</article>
	<%- partial('pagination') %>
	<%- partial('comment') %>
</div>